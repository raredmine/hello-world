#!/usr/bin/perl -w
use strict;
use List::Util qw(shuffle);
use 5.010;

print randStr(8)."\n";
sub randStr {
    return join('', map{ ('a'..'z','A'..'Z',0..9, '_' )[rand 62]} 0..$_[0]);
}


#my @shuffled = shuffle(1..10);

my $rand = map {('a'..'z')[rand 62]} 0..8;

print $rand;
#print @shuffled;

#print "Some powers of two are:\n",
#map "\t" . ( 2 ** $_ ) . "\n", 0..32;

srand( time() ^ ($$ + ($$ << 15)) );
#print rand(6);
my @v = qw ( a e i o u y );
my @c = qw ( b c d f g h j k l m n p q r s t v w x z );

for (1..12) {
    my ($flip, $str) = (0,'');
    $str .= ($flip++ % 2) ? $v[rand(6)] : $c[rand(20)] for 1 .. 9;
    $str =~ s/(....)/$1 . int rand(10)/e;
    $str = ucfirst $str if rand() > 0.5;
    print "$str\n";
}
